{
  "name": "enabler",
  "private": true,
  "version": "4.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n VITE,TSC -c magenta,cyan \"vite --port 3000\" \"npm run tsc:dev\"",
    "docker:dev": "concurrently -n VITE,TSC -c magenta,cyan \"vite --host 0.0.0.0 --port 3000\" \"npm run tsc:dev\"",
    "tsc:dev": "tsc --noEmit --watch --preserveWatchOutput --pretty",
    "start": "serve public -l 8080 --cors",
    "test": "jest --detectOpenHandles",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "npm run build && serve public -l 3000 --cors"
  },
  "dependencies": {
    "braintree-web-drop-in": "^1.44.1",
    "serve": "14.2.4"
  },
  "devDependencies": {
    "@types/braintree-web-drop-in": "^1.39.3",
    "@types/node": "22.14.1",
    "concurrently": "^9.1.2",
    "jest": "29.7.0",
    "sass": "1.87.0",
    "ts-jest": "^29.3.2",
    "typescript": "5.8.3",
    "vite": "6.3.3",
    "vite-plugin-css-injected-by-js": "3.5.2"
  },
  "overrides": {
    "path-to-regexp": "3.3.0",
    "esbuild": "^0.24.0"
  }
}
